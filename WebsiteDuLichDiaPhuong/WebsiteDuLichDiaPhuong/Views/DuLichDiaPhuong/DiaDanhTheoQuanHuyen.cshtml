@model IEnumerable<WebsiteDuLichDiaPhuong.Models.DIADANH>
@{
    ViewBag.Title = "DiaDanhTheoQuanHuyen";
    Layout = "~/Views/Shared/_LayoutNguoiDung.cshtml";
}

<h2 style="text-align: center; color: brown"> ĐỊA DANH THEO QUẬN HUYỆN</h2>
<div class="container-fluid pt-5">
    <div class="row px-xl-5 pb-3">
        @foreach (var item in Model)
        {
            <div class="col-lg-4 col-md-6 pb-1">
                <div class="cat-item d-flex flex-column border mb-4" style="padding: 30px;">
                    <a href="@Url.Action("ChiTiet", "DuLichDiaPhuong", new {id = item.MaHuyen})" class="cat-img position-relative overflow-hidden mb-3">
                        <img class="img-fluid" src="~/Images/@item.HINHANH.TenHinhAnh" style="width: 400px; height: 250px" alt="">
                    </a>
                    <p class="text-center">@item.TenDiaDanh</p>
                </div>
            </div>
        }
    </div>
</div>

